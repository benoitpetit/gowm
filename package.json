{
    "name": "gowm",
    "version": "1.0.7",
    "description": "Go WebAssembly Manager - Simplify integration of Go WebAssembly modules in your JavaScript projects with GitHub repository support, React hooks, and Vue composables",
    "main": "src/index.js",
    "browser": "src/browser.js",
    "module": "src/browser.js",
    "types": "types/index.d.ts",
    "exports": {
        ".": {
            "node": "./src/index.js",
            "browser": "./src/browser.js",
            "import": "./src/browser.js",
            "require": "./src/index.js"
        },
        "./hooks": "./hooks/useWasm.js",
        "./composables": "./composables/useWasm.js"
    },
    "files": [
        "src/",
        "types/",
        "hooks/",
        "composables/",
        "runtime/",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "test": "node examples/node/basic.js",
        "test:basic": "node examples/node/basic.js",
        "test:crypto": "node examples/node/crypto.js",
        "check": "node check-project.js",
        "demo:serve": "npx serve examples/browser -p 3000",
        "demo:browser": "npm run demo:serve && echo 'Open http://localhost:3000'",
        "build": "echo 'No build step needed for this package'",
        "prepublishOnly": "npm run check"
    },
    "keywords": [
        "wasm",
        "webassembly",
        "go",
        "golang",
        "javascript",
        "nodejs",
        "react",
        "vue",
        "vuejs",
        "github",
        "bridge",
        "loader",
        "manager",
        "typescript"
    ],
    "author": "devbyben",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/benoitpetit/gowm.git"
    },
    "bugs": {
        "url": "https://github.com/benoitpetit/gowm/issues"
    },
    "homepage": "https://github.com/benoitpetit/gowm#readme",
    "engines": {
        "node": ">=14.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0"
    },
    "dependencies": {
        "node-fetch": "^3.3.2"
    }
}
